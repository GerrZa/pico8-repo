	update=--‚òÖ
	function(s)
	 s:uinit()
		s.it+=1
		
		if s.state=="i" then
		 if (s.ii<0) s.state="pc"
		 for pc in all(s.pcard) do
		 	pc.yoff=0
		 end
		elseif s.state=="i2" then
		 if (s.ii<1) s.state="pc"
		 for pc in all(s.pcard) do
		 	pc.yoff=0
		 end
		elseif s.state=="pc" then
			--select card
			if btnp(‚¨ÖÔ∏è) and s.psel>1 then
				s.psel-=1
				sfx(0)
			elseif btnp(‚û°Ô∏è) and s.psel<#s.pcard then
				s.psel+=1
				sfx(0)
			end
			
			s.ptsel=#s.ptok
			
			for i=1,#s.pcard do
				if i==s.psel then
					s.pcard[i].yoff=-8
				else
					s.pcard[i].yoff=0
				end
			end
			
			for i=1,#s.ptok do
				s.ptok[i].xoff=0
			end
		
			if btnp(‚ùé) then
			 s.state="pt"
			end
		
		---
		elseif s.state=="pt" then
			
			if btnp(‚¨áÔ∏è) and s.ptsel>1 then
				s.ptsel-=1
				sfx(0)
			elseif btnp(‚¨ÜÔ∏è) and s.ptsel<#s.ptok then
				s.ptsel+=1
				sfx(0)
			end
			
			if btnp(üÖæÔ∏è) then
				s.state="pc"
				sfx(2)
			end
			
			for i=1,#s.ptok do
				if i==s.ptsel then
					s.ptok[i].xoff=10
				else
					s.ptok[i].xoff=0
				end
			end
			
			if btnp(‚ùé) then
				sfx(3)
				local holdval=s.pcard[s.psel].val
				local holdcol=11
				local holdtxt="+"..holdval
				if s.ptsel!=#s.ptok then
					holdval*=-1
					holdcol=8
					holdtxt=holdval
					deli(s.ptok,s.ptsel)
				end
				s.stack+=holdval
				del(s.pcard,s.pcard[s.psel])
				s:addcard(false)
				s:explode()
			 s.camsh=5
				s.psel=1
				s.ptsel=#s.ptok
				if s.stack==21 then
				 s:addtxt("perfect!",50,62,11)
					s:downhp(true)
				 s.state="ng"
				 sfx(4)
				elseif s.stack<0 or s.stack>21 then
					s.state="ng"
				 s:downhp(false)
					s:addtxt("aww:(",58,60,8)
					sfx(6)
				else
					s.state="e"
					s.etime=20+ceil(rnd(2*30))
					s:addtxt(holdtxt,60,60,holdcol)
				end
			end
		---
		elseif s.state=="e" then
			for i=1,#s.pcard do
				s.pcard[i].ay=106
			end
			
			for i=1,#s.ptok do
				s.ptok[i].xoff=0
			end
			
			s.etime-=1
			if s.etime<1 then
				s:eneplace()
			end
		elseif s.state=="ng" then
			for t in all(s.ptok) do
				t.xoff=0
			end
		
			if s.php==0 or s.ehp==0 then
			 s.gof-=1
			 if (s.ehp<=0 and s.gof<0) then
				 local newgo=gameover:new({win="p"})
--				 newgo.win="p"
				 scene=newgo
			 elseif (s.php<=0 and s.gof<0) then
				 local newgo=gameover:new({win="e"})
--				 newgo.win="e"
				 scene=newgo
				end
			else
				if btnp(‚ùé) or btnp(üÖæÔ∏è) then
					s.state="i2"
					s.ii=3
					s.it=0
					s.pcard={}
					s.ecard={}
					s.stack=0
				end
			end
		end
		//end of state
		
		//card pos
		for i=0,#s.pcard-1 do
		 local pc=s.pcard[i+1]
			pc.x=2+(i*16)
			pc.ax+=(pc.x-pc.ax)*0.4
			pc.ay+=((pc.y+pc.yoff)-pc.ay)*0.4
		end
		
		for i=0,#s.ecard-1 do
		 local ec=s.ecard[i+1]
			ec.x=102-(i*8)
			ec.ax+=(ec.x-ec.ax)*0.4
			ec.ay+=((ec.y+ec.yoff)-ec.ay)*0.4
		end
		
		--tok pos
		for i=1,#s.ptok do
			local pt=s.ptok[i]
			pt.y=124-(10*i)
			pt.ax+=((pt.x-pt.xoff)-pt.ax)*0.4
			pt.ay+=(pt.y-pt.ay)*0.4
		end
		
		--circ part
		
		local ncirc={x=64,y=64,l=8+rnd(10),dir=rnd(1),s=0.5,r=6+rnd(4),c=7}
		ncirc.x+=cos(ncirc.dir)*14
		ncirc.y+=sin(ncirc.dir)*14
		add(s.cenpart,ncirc)
		
		if #s.cenpart>1 then
			for i=#s.cenpart,2,-1 do
				local cp = s.cenpart[i]
				cp.x+=cos(cp.dir)*cp.s
				cp.y+=sin(cp.dir)*cp.s
				cp.l-=1
				cp.s=cp.s*0.995
				if cp.l<=0 then
					del(s.cenpart,cp)
				end
			end
		end
		
		s.camsh-=1
		
	end

