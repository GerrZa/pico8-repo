	explode=
	function(s)
		for i=1,4+ceil(rnd(3)) do
		 local ne = {x=64,y=64,r=3+rnd(4),dir=rnd(1),s=4,l=10+ceil(rnd(5)),c=8}
		 ne.x+=cos(ne.dir)*4
		 ne.y+=sin(ne.dir)*8
			add(s.cenpart,ne)
		end
	end,
	
	spark=
	function(s,sx,sy,sc)
		for i=1,4+ceil(rnd(2)) do
			local ns={
				x=sx,
				y=sy,
				c=sc,
				ln=ceil(rnd(4)),
				l=5+ceil(rnd(3)),
				d=rnd(1),
				s=2
			}
			add(s.spfx,ns)
		end
	end,
	
	downhp=
	function(s,ene)
		if ene then
			s.ehp-=1
			s:spark(25+s.ehp*6,2,8)
		else
			s.php-=1
			s:spark(25+s.php*6,10,9)
		end
	end,
	
