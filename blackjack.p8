gscene = {
	state="i",
	fade=0,
	it=-20, --inittime
	ii=3, --initcard
	stack=0,
	pcard={},
	ecard={},
	etok={},
	ptok={},
	psel=1,
	etime=1+rnd(3*30),
	ptsel=1,
	phold=0,
	partcd=5,
	camsh=0,
	php=5,
	ehp=5,
	txtfx={},
	lastenemove="dumb",
	cenpart={
		{x=64,y=64,r=20}
	},
	spfx={},
	gof=30,
	new=
	function(self,o)
	 o = o or {}
	 setmetatable(o, {__index=self})
	 self.pcard={}
	 self.ecard={}
	 self.ptok={}
	 self.etok={}
	 self.cenpart={{x=64,y=64,r=20}}
	 return o
	end,
	
	explode=
	function(s)
		for i=1,4+ceil(rnd(3)) do
		 local ne = {x=64,y=64,r=3+rnd(4),dir=rnd(1),s=4,l=10+ceil(rnd(5)),c=8}
		 ne.x+=cos(ne.dir)*4
		 ne.y+=sin(ne.dir)*8
			add(s.cenpart,ne)
		end
	end,
	
	spark=
	function(s,sx,sy,sc)
		for i=1,4+ceil(rnd(2)) do
			local ns={
				x=sx,
				y=sy,
				c=sc,
				ln=ceil(rnd(4)),
				l=5+ceil(rnd(3)),
				d=rnd(1),
				s=2
			}
			add(s.spfx,ns)
		end
	end,
	
	downhp=
	function(s,ene)
		if ene then
			s.ehp-=1
			s:spark(25+s.ehp*6,2,8)
		else
			s.php-=1
			s:spark(25+s.php*6,10,9)
		end
	end,
	
	addtxt=
	function(s,txt,tx,ty,tc)
		local txtf={
			t=txt,
			x=tx,
			y=ty,
			c=tc,
			l=10,
			s=2
		}
		add(s.txtfx,txtf)
	end,
	
	addcard=
	function(s,ene)
		if ene then
	 	local nc={
		 	x=0,
		 	y=-24,
		 	ax=0,
		 	ay=-24,
		 	yoff=0,
		 	val=ceil(rnd(10))
		 }
	 	add(s.ecard,nc)
			sfx(5)
	 else
			local ncard={
				x=0,
				y=106,
				ax=128,
				ay=106,
				yoff=0,
				val=ceil(rnd(10)),
			}
			add(s.pcard,ncard)
		end
	end,
	
	addtok=
	function(s,ene,p)
		local tok={val=sgn(1-rnd(2))}
		if (p) tok.val=1
		if tok.val==1 then
			tok.s=39
		else
			tok.s=7
		end
		if ene then
		 add(s.etok,tok)
		else
			tok.x=120
			tok.y=110
			tok.ax=120
			tok.ay=110
			tok.xoff=0
		 add(s.ptok,tok)
		end
	end,
	
	eaddstack=
	function(s,val)
	  s.stack+=val
	  s:explode()
	  s.camsh=5
	  if s.stack==21 then
	   s.state="ng"
			 s:downhp(false)
			 s:addtxt("perfect!",50,62,11)
			 sfx(6)
			elseif s.stack>-1 and s.stack<22 then
				s.state="pc"
				local at=val
				local atc=8
				if val>0 then
					at="+"..val
					atc=11
				end
				s:addtxt(at,60,60,atc)
			else
			 s:downhp(true)
				s.state="ng"
				s:addtxt("aww:(",58,60,8)
				sfx(4)
			end
	end,
	
	eneplace=
	function(s)
	 local smch=rnd(1)
	 if smch<=dif[globedif] then --being smart
	  s.lastenemove="smart"
	  local ssmart=false
	  if (rnd(1)>0.5 and globedif==3) ssmart=true
	  if count(s.etok,{val=1})==3 then
	  	local place={0,{val=0}}
	  	local stragp={}
	  	local cheat={}
	  	for crd in all(s.ecard) do
	 		 local sumval=s.stack+crd.val
	  		if sumval==21 then
	  		 place[1]=crd
	  		end
	  		if sumval<22 then
	  		 if sumval>s.stack+place[2].val then
		  		 place[2]=crd
	  		 end
	  		 
	  		 if ssmart then
		  		 local makego=0
		  		 for pc in all(s.pcard) do
		  		  if sumval+pc.val>21 then
									makego+=1
									if makego==3 then
									 add(cheat,crd)
									 break
									end
		  		  end
		  		 end
		  		end
	  		end
	  		if sumval<11 and sumval>0 and globedif==3 then
	  		 add(stragp,crd)
	  		end
	  	end
	  	--place
	  	if place[1]!=0 then
	  	 s:eaddstack(place[1].val)
	  	 del(s.ecard,place[1])
	  	 deli(s.etok,1)
	  	 s:addcard(true)
	  	 s:addtok(true)
	  	elseif #cheat>0 then
	  	 local holdc=cheat[ceil(rnd(#cheat))]
	  	 s:eaddstack(holdc.val,s.etok[1])
	  	 del(s.ecard,holdc)
	  	 deli(s.etok,1)
	  	 s:addcard(true)
	  	 s:addtok(true)
	  	elseif #stragp>0 then
	  	 local holdc=stragp[ceil(rnd(#stragp))]
	  	 s:eaddstack(holdc.val,s.etok[1])
	  	 del(s.ecard,holdc)
	  	 deli(s.etok,1)
	  	 s:addcard(true)
	  	 s:addtok(true)
	  	elseif place[2]!={val=0} then
	  	 s:eaddstack(place[2].val)
	  	 del(s.ecard,place[2])
	  	 deli(s.etok,1)
	  	 s:addcard(true)
	  	 s:addtok(true)
	  	else
	  	 local holdc=s.ecard[ceil(rnd(3))]
	  		s:eaddstack(holdc.val)
	  		del(s.ecard,holdc)
	  		deli(s.etok,1)
	  	 s:addcard(true)
	  	 s:addtok(true)
	  	end
	  ------
	  elseif count(s.etok,{val=-1})==3 then
	   local place={}
	   local cheat={}
	  	for crd in all(s.ecard) do
	 		 local sumval=s.stack-crd.val
	  		if sumval>-1 then
	  		 add(place,crd)
	  		 local makego=0
	  		 for pcrd in all(s.pcard) do
	  		  if sumval-pcrd.val<0 then
	  		  	makego+=1
	  		  	if makego==3 then
	  		  	 add(cheat,crd)
	  		  		break
	  		  	end
	  		  end
	  		 end
	  		end
	  	end
	  	
	  	if (#place<1) place=s.pcard
	  	
	  	if #cheat>0 then
	  		local holdc=cheat[ceil(rnd(#cheat))]
	  		s:eaddstack(holdc.val)
		  	del(s.ecard,holdc)
		  	deli(s.etok,1)
	  	 s:addcard(true)
	  	 s:addtok(true)
	  	else
		  	local chosecard=place[ceil(rnd(#place))]
		  	s:eaddstack(chosecard.val)
		  	del(s.ecard,chosecard)
		  	deli(s.etok,1)
	  	 s:addcard(true)
	  	 s:addtok(true)
	  	end
	  	
	  else
	   local perfect={}
	   local place={}
	  	for crd in all(s.ecard) do
	  		for t in all(s.etok) do
	  		 local sumval=s.stack+(crd.val*t.val)
	  		 if sumval==21 then
	  		  add(perfect,{crd,t})
	  		 elseif sumval>-1 and sumval<22 then
	  		  add(place,{crd,t})
	  		 end
	  		end
	  	end
	  	
	  	if #perfect>0 then
	  	 local holdmat=perfect[ceil(rnd(#perfect))]
	  	 s:eaddstack(holdmat[1].val*holdmat[2].val)
	  	 del(s.ecard,holdmat[1])
	  	 del(s.etok,holdmat[2])
	  	 s:addcard(true)
	  	 s:addtok(true)
	  	elseif #place>0 then
	  	 local holdmat=place[ceil(rnd(#place))]
	  	 s:eaddstack(holdmat[1].val*holdmat[2].val)
	  	 del(s.ecard,holdmat[1])
	  	 del(s.etok,holdmat[2])
	  	 s:addcard(true)
	  	 s:addtok(true)
	  	else
	  	 local holdmat={s.ecard[ceil(rnd(#s.ecard))],s.etok[ceil(rnd(#s.etok))]}
	  	 s:eaddstack(holdmat[1].val*holdmat[2].val)
	  	 del(s.ecard,holdmat[1])
	  	 del(s.etok,holdmat[2])
	  	 s:addcard(true)
	  	 s:addtok(true)
	  	end
	  end
	 else
	  s.lastenemove="dumb"
	  local ehold=0
	  local chold=s.ecard[ceil(rnd(3))]
	  local thold=s.etok[ceil(rnd(3))]
	  ehold=chold.val*thold.val
	  del(s.ecard,chold)
	  del(s.etok,thold)
	  s:addcard(true)
	  s:addtok(true)
	  s:eaddstack(ehold)
	 end
	end,
	
	ufade=
	function(s)
		if s.fade<12 then
		 for k,v in pairs(pall) do
		  pal(k,pall[k][1+(s.fade\3)])
		 end
		else
		 pal()
		end
	end,
	
	uinit=
	function(s)
	 if (s.fade<12) s.fade+=1
	 if s.it > 5 and s.it%6==0 and s.ii>0 then
	 	s:addcard(false)
	 	s:addcard(true)
	 	local addp=false
	 	if #s.ptok>1 then
		 	for t in all(s.ptok) do
		 		if (t.val<0) addp=true
		 	end
		 end
	 	if addp then
		 	s:addtok(false,true)
	 	else
		 	s:addtok(false)
	 	end
	 	s:addtok(true)
	 	s.ii-=1
	 end
	end,
	
	draw=
	function(s)--üê±
	 s:ufade()
		cls()
		if s.state!="ng" and s.state!="i" then
			cursor(88,20,1)
		 print("confirm-‚ùé")
		 print("   back-üÖæÔ∏è")
			local difn={"easy","normal","hard"}
			print(difn[globedif],1,20,1)
		end
		
		local sstate=sub(s.state,1,1)
		if sstate=="p" then
			print("‚¨áÔ∏èyourturn‚¨áÔ∏è",41,92,11)
		elseif sstate=="e" then
			print("‚¨ÜÔ∏èenemyturn‚¨ÜÔ∏è",38,30,8)
		elseif s.state=="ng" and s.php>0 and s.ehp>0 then
			print("üÖæÔ∏ènextround‚ùé",38,92,9)
		end
		//end of state
		
		--drawcard
		for pc in all(s.pcard) do
			pc.ay=ceil(pc.ay)
			spr(1,pc.ax,pc.ay,3,4)
			local pcay=pc.ay
			if pc.val==1 then
				print("a",pc.ax+2,pc.ay+1,8)
			elseif pc.val==10 then
				print("k",pc.ax+2,pc.ay+1,8)
			else
				print(pc.val,pc.ax+2,pc.ay+1,8)
			end
		end
		
		for es in all(s.ecard) do
			spr(4,es.ax,es.ay,3,4)
		end
		
		--drawtok
		for pt in all(s.ptok) do
			pt.ax=ceil(pt.ax)
			spr(pt.s,pt.ax,pt.ay,2,2)
		end
		
		--circ part
		if #s.cenpart > 1 then
			for i=2,#s.cenpart do
				local cp=s.cenpart[i]
				if cp.l > 5 then
					fillp(‚ñà)
					circfill(cp.x,cp.y,cp.r,cp.c)
				else
					if cp.c==8 then
						cp.c=2
					elseif cp.c==7 then
						cp.c=13
					end
					fillp(‚ñí)
					if cp.l < 3 then
						fillp(‚ñë)
						cp.c=1
					end
					circfill(cp.x,cp.y,cp.r,cp.c)
				end
			end
		end
		
		fillp(‚ñà)
		local cen=s.cenpart[1]
		circfill(cen.x,cen.y,cen.r,7)
		
		local tc=0
		if (s.stack/21)>1 or (s.stack/21)<0 then
			tc=8
		elseif s.stack==21 then
			tc=11
		elseif (s.stack/21)>0.9 then
			tc=9
		elseif (s.stack/21)>0.7 then
			tc=2
		end
		
		if s.stack<0 then
			if s.stack<-9 then
				print(s.stack.."/21",52,62,tc)
			else
				print(s.stack.."/21",54,62,tc)
			end
		elseif s.stack>9 then
			print(s.stack.."/21",55,62,tc)
		else
			print(s.stack.."/21",57,62,tc)
		end
		
		for sp in all(s.spfx) do
			line(sp.x,sp.y,sp.x+(cos(sp.d)*sp.ln),sp.y+(sin(sp.d)*sp.ln),sp.c)
			sp.x+=cos(sp.d)*sp.s
			sp.y+=sin(sp.d)*sp.s
			sp.s*=0.8
			sp.l-=1
			if (sp.l<0) del(s.spfx,sp)
		end
		
		--hp stat
		cursor(0,0,13)
		print("enemy:",1,1)
		print("  you:",1,8)
		
		for i=0,s.ehp-1 do
		 print("‚ô•",24+(i*6),1,8)
		end
		for i=0,s.php-1 do
		 print("‚ô•",24+(i*6),8,9)
		end
		
		for t in all(s.txtfx) do
			if (t.l<0) del(s.txtfx,t)
			print(t.t,t.x,t.y,t.c)
			t.l-=1
			t.y-=t.s
			t.s*=0.9
		end
		
		if s.camsh>0	then
			camera(-2+rnd(4),-2+rnd(4))
		else
			camera(0,0)
		end
		
		
	 cursor(1,30,7)
		for i in all(s.etok) do
		 print(i.val)
		end
	end,
	
	update=--‚òÖ
	function(s)
	 s:uinit()
		s.it+=1
		
		if (s.it>5000) s.it=0
		
		if s.state=="i" then
		 if (#s.pcard>2) s.state="pc"
		 for pc in all(s.pcard) do
		 	pc.yoff=0
		 end
		elseif s.state=="pc" then
			--select card
			if btnp(‚¨ÖÔ∏è) and s.psel>1 then
				s.psel-=1
				sfx(0)
			elseif btnp(‚û°Ô∏è) and s.psel<#s.pcard then
				s.psel+=1
				sfx(0)
			end
			
			for i=1,#s.pcard do
				if i==s.psel then
					s.pcard[i].yoff=-8
				else
					s.pcard[i].yoff=0
				end
			end
			
			for i=1,#s.ptok do
				s.ptok[i].xoff=0
			end
		
		if btnp(‚ùé) then
			s.phold={s.pcard[s.psel],s.pcard[s.psel].val}
			s.state="pt"
			sfx(1)
		end
		
		---
		elseif s.state=="pt" then
			
			if btnp(‚¨áÔ∏è) and s.ptsel>1 then
				s.ptsel-=1
				sfx(0)
			elseif btnp(‚¨ÜÔ∏è) and s.ptsel<#s.ptok then
				s.ptsel+=1
				sfx(0)
			end
			
			if btnp(üÖæÔ∏è) then
				s.state="pc"
				sfx(2)
			end
			
			for i=1,#s.ptok do
				if i==s.ptsel then
					s.ptok[i].xoff=10
				else
					s.ptok[i].xoff=0
				end
			end
			
			if btnp(‚ùé) then
				local holdv=s.phold[2]*s.ptok[s.ptsel].val
				s.camsh=5
				s:explode()
				s.stack+=holdv
				del(s.pcard,s.phold[1])
				del(s.ptok,s.ptok[s.ptsel])
				s:addcard(false)
				s:addtok(false)
				s.psel=1
				s.ptsel=1
				if s.stack==21 then
				 s:downhp(true)
				 s:addtxt("perfect!",50,62,11)
				 s.state="ng"
				 sfx(4)
				elseif s.stack>-1 and s.stack<22 then
					s.state="e"
					s.etime=20+ceil(rnd(2*30))
					local at="-"..s.phold[2]
					local atc=8
					if holdv>0 then
						at="+"..s.phold[2]
						atc=11
					end
					s:addtxt(at,60,60,atc)
				else
					s.state="ng"
				 s:downhp(false)
					s:addtxt("aww:(",58,60,8)
					sfx(6)
				end
				sfx(3)
			end
		---
		elseif s.state=="e" then
			for i=1,#s.pcard do
				s.pcard[i].ay=106
			end
			
			for i=1,#s.ptok do
				s.ptok[i].xoff=0
			end
			
			s.etime-=1
			if s.etime<1 then
				s:eneplace()
				sfx(3)
			end
		elseif s.state=="ng" then
			if s.php==0 or s.ehp==0 then
			 s.gof-=1
			 if (s.ehp<=0 and s.gof<0) then
				 local newgo=gameover:new({win="p"})
--				 newgo.win="p"
				 scene=newgo
			 elseif (s.php<=0 and s.gof<0) then
				 local newgo=gameover:new({win="e"})
--				 newgo.win="e"
				 scene=newgo
				end
			else
				if btnp(‚ùé) or btnp(üÖæÔ∏è) then
					s.state="i"
					s.ii=3
					s.it=0
					s.pcard={}
					s.ptok={}
					s.etok={}
					s.ecard={}
					s.stack=0
				end
			end
		end
		//end of state
		
		//card pos
		for i=0,#s.pcard-1 do
		 local pc=s.pcard[i+1]
			pc.x=2+(i*16)
			pc.ax+=(pc.x-pc.ax)*0.4
			pc.ay+=((pc.y+pc.yoff)-pc.ay)*0.4
		end
		
		for i=0,#s.ecard-1 do
		 local ec=s.ecard[i+1]
			ec.x=102-(i*8)
			ec.ax+=(ec.x-ec.ax)*0.4
			ec.ay+=((ec.y+ec.yoff)-ec.ay)*0.4
		end
		
		--tok pos
		for i=1,#s.ptok do
			local pt=s.ptok[i]
			pt.y=124-(10*i)
			pt.ax+=((pt.x-pt.xoff)-pt.ax)*0.4
			pt.ay+=(pt.y-pt.ay)*0.4
		end
		
		--circ part
		
		local ncirc={x=64,y=64,l=8+rnd(10),dir=rnd(1),s=0.5,r=6+rnd(4),c=7}
		ncirc.x+=cos(ncirc.dir)*14
		ncirc.y+=sin(ncirc.dir)*14
		add(s.cenpart,ncirc)
		
		
		if #s.cenpart>1 then
			for i=#s.cenpart,2,-1 do
				local cp = s.cenpart[i]
				cp.x+=cos(cp.dir)*cp.s
				cp.y+=sin(cp.dir)*cp.s
				cp.l-=1
				cp.s=cp.s*0.995
				if cp.l<=0 then
					del(s.cenpart,cp)
				end
			end
		end
		
		s.camsh-=1
		
	end

}
